<table class="table table-hover">
  <thead>
    <tr>
      <th>Nickname</th>
      <th>Email</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Birthday</th>
    </tr>
  </thead>
  <tbody>
    {{#each user in model}}
      <tr class="clickable" {{action 'show' user}}>
        <td>{{user.nickname}}</td>
        <td>{{user.email}}</td>
        <td>{{user.firstName}}</td>
        <td>{{user.lastName}}</td>
        <td>{{format-time user.birthday format='D MMM YYYY'}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
